#include "MCBooster.h"

MCBooster::MCBooster()
{
    //boost

}

MCBooster::~MCBooster()
{
    //boost
}

{
    //Kill Mobs

//Kill Creepers
    if config.killcreep = true;
        then this.kill(EntityCreeper);
    if config.killcreep = false;
        then this.ignore(EntityCreeper);

//Kill Sheeps
    if config.killsheep = true;
        then this.kill(EntitySheep);
    if config.killsheep = false;
        then this.ignore(EntitySheep);

//Kill Cows
    if config.killcow = true;
        then.this.kill(EntityCow);
    if config.killcow = false;
        then.this.ignore(EntityCow);

//Kill Chickens
    if config.killchicken = true;
        then.this.kill(EntityChicken);
    if config.killchicken = false;
        then.this.ignore(EntityChicken);
}
